{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "MIT",
  "scripts": {
    "watch:css": "npx tailwindcss -o ./app/styles/tailwind.css --watch",
    "build:css": "npx tailwindcss -o ./app/styles/tailwind.css",
    "watch": "remix watch",
    "build": "npm run test-tsc && remix build",
    "dev": "remix dev",
    "test-tsc": "tsc --noEmit",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,md,css,json}\" --ignore-path .gitignore",
    "format:quick": "pretty-quick --staged --pattern \"**/*.{js,jsx,ts,tsx,md,css,json}\"",
    "lint": "eslint . --ext .ts,.js,.tsx,.jsx",
    "start": "remix-serve api/build",
    "prepare": "husky install"
  },
  "dependencies": {
    "@prisma/client": "^4.10.1",
    "@radix-ui/react-dialog": "^1.0.2",
    "@radix-ui/react-dropdown-menu": "^2.0.2",
    "@radix-ui/react-id": "^1.0.0",
    "@radix-ui/react-portal": "^1.0.1",
    "@radix-ui/react-switch": "^1.0.1",
    "@radix-ui/react-toolbar": "^1.0.2",
    "@radix-ui/react-tooltip": "^1.0.3",
    "@react-aria/label": "^3.5.0",
    "@react-aria/ssr": "^3.5.0",
    "@remix-run/node": "^1.13.0",
    "@remix-run/react": "^1.13.0",
    "@remix-run/serve": "^1.13.0",
    "@remix-run/vercel": "^1.13.0",
    "bcrypt": "^5.1.0",
    "classnames": "^2.3.2",
    "is-url": "^1.2.4",
    "jsonwebtoken": "^9.0.0",
    "nprogress": "^0.2.0",
    "react": "^18.2.0",
    "react-aria": "^3.23.0",
    "react-bootstrap-icons": "^1.10.2",
    "react-dom": "^18.2.0",
    "react-hotkeys-hook": "^4.3.7",
    "react-router": "^6.8.1",
    "react-router-dom": "6.8.1",
    "remix-utils": "^6.0.0",
    "slate": "^0.91.4",
    "slate-history": "^0.86.0",
    "slate-react": "^0.91.4",
    "zod": "^3.20.6",
    "zustand": "^4.3.3"
  },
  "devDependencies": {
    "@casper124578/eslint-config": "^6.0.0",
    "@casper124578/eslint-config-react": "^6.0.0",
    "@remix-run/dev": "^1.13.0",
    "@remix-run/server-runtime": "^1.13.0",
    "@types/bcrypt": "^5.0.0",
    "@types/is-url": "^1.2.30",
