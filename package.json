    "@remix-run/server-runtime": "^0.20.1",
    "@types/bcrypt": "^5.0.0",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/react": "^17.0.34",
    "@types/react-dom": "^17.0.11",
    "autoprefixer": "^10.4.0",
    "eslint": "^7.32.0",
    "postcss": "^8.3.11",
    "postcss-cli": "^9.0.2",
    "tailwindcss": "^2.2.19",
    "type-fest": "^2.5.3",
