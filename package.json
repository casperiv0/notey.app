{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "scripts": {
    "clean": "rimraf ./node_modules/.cache ./server/dist ./build ./public/build",
    "prebuild": "npm run clean && echo All clean âœ¨",
    "watch:css": "postcss styles --base styles --dir app/styles -w",
    "build:css": "postcss styles --base styles --dir app/styles --env production",
    "build": "remix build",
    "dev:remix": "remix dev",
    "dev": "concurrently \"npm run watch:css\" \"npm run dev:remix\"",
    "test-tsc": "tsc --noEmit",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,md,css,json}\" --ignore-path .gitignore",
    "lint": "eslint . --ext .ts,.js,.tsx,.jsx",
    "start": "remix-serve build"
  },
  "dependencies": {
    "@prisma/client": "^3.4.2",
    "@radix-ui/react-dialog": "^0.1.1",
    "@radix-ui/react-dropdown-menu": "^0.1.1",
    "@react-aria/utils": "^3.9.0",
    "@remix-run/react": "^0.20.1",
    "@remix-run/serve": "^0.20.1",
    "bcrypt": "^5.0.1",
    "classnames": "^2.3.1",
    "concurrently": "^6.4.0",
    "formik": "^2.2.9",
    "jsonwebtoken": "^8.5.1",
    "prisma": "^3.4.2",
    "react": "^17.0.2",
    "react-aria": "^3.10.0",
    "react-bootstrap-icons": "^1.6.1",
    "react-dom": "^17.0.2",
    "react-router-dom": "6.0.0-beta.8",
    "remix": "^0.20.1",
