    "@types/node": "^18.14.1",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.6.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.34.0",
    "husky": "^8.0.3",
    "postcss": "^8.4.21",
    "postcss-cli": "^10.1.0",
    "prettier": "^2.8.4",
    "pretty-quick": "^3.1.3",
    "prisma": "^4.10.1",
    "tailwindcss": "^3.2.7",
