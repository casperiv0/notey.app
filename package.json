{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "MIT",
  "scripts": {
    "watch:css": "npx tailwindcss -o ./app/styles/tailwind.css --watch",
    "build:css": "npx tailwindcss -o ./app/styles/tailwind.css",
    "watch": "concurrently \"npm run watch:css\" \"remix watch\"",
    "build": "npm run test-tsc && npm run build:css && remix build",
    "dev:remix": "remix dev",
    "dev": "concurrently \"npm run watch:css\" \"npm run dev:remix\"",
    "test-tsc": "tsc --noEmit",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,md,css,json}\" --ignore-path .gitignore",
    "format:quick": "pretty-quick --staged --pattern \"**/*.{js,jsx,ts,tsx,md,css,json}\"",
    "lint": "eslint . --ext .ts,.js,.tsx,.jsx",
    "start": "remix-serve api/build",
    "prepare": "husky install"
  },
  "dependencies": {
    "@prisma/client": "^4.3.0",
    "@radix-ui/react-dialog": "^1.0.0",
    "@radix-ui/react-dropdown-menu": "^1.0.0",
    "@radix-ui/react-id": "^1.0.0",
    "@radix-ui/react-portal": "^1.0.0",
    "@radix-ui/react-switch": "^1.0.0",
    "@radix-ui/react-toolbar": "^1.0.0",
    "@radix-ui/react-tooltip": "^1.0.0",
    "@react-aria/label": "^3.4.1",
    "@react-aria/ssr": "^3.3.0",
    "@remix-run/node": "^1.7.0",
    "@remix-run/react": "^1.7.0",
    "@remix-run/serve": "^1.7.2",
    "@remix-run/vercel": "^1.7.0",
    "bcrypt": "^5.0.1",
    "classnames": "^2.3.1",
    "is-url": "^1.2.4",
    "jsonwebtoken": "^8.5.1",
    "nprogress": "^0.2.0",
    "react": "^18.2.0",
    "react-aria": "^3.19.0",
    "react-bootstrap-icons": "^1.8.4",
    "react-dom": "^18.2.0",
    "react-hotkeys-hook": "^3.4.7",
    "react-router": "^6.3.0",
    "react-router-dom": "6.3.0",
    "remix-utils": "^3.3.0",
    "slate": "^0.82.1",
    "slate-history": "^0.66.0",
    "slate-react": "^0.82.1",
    "zod": "^3.18.0",
    "zustand": "^4.1.1"
  },
  "devDependencies": {
    "@casper124578/eslint-config": "^4.4.0",
    "@casper124578/eslint-config-react": "^4.4.0",
    "@remix-run/dev": "^1.7.0",
    "@remix-run/server-runtime": "^1.7.0",
    "@types/bcrypt": "^5.0.0",
    "@types/is-url": "^1.2.30",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.7.14",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "autoprefixer": "^10.4.8",
    "concurrently": "^7.3.0",
    "dotenv": "^16.0.2",
    "eslint": "^8.23.0",
    "husky": "^8.0.1",
    "postcss": "^8.4.16",
    "postcss-cli": "^10.0.0",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "prisma": "^4.3.0",
    "tailwindcss": "^3.1.8",
    "type-fest": "^2.19.0",
    "typescript": "^4.8.2"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
