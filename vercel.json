{
  "build": {
    "env": {
      "ENABLE_FILE_SYSTEM_API": "1",
      "VERCEL_BUILD_CLI_PACKAGE": "vercel@canary"
    }
  },
  "headers": [
    {
      "source": "/build/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000"
        }
      ]
    }
  ]
}
