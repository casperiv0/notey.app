{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": ["@casper124578/eslint-config", "@casper124578/eslint-config-react"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "no-duplicate-imports": "off",
    "@typescript-eslint/ban-types": "off"
  }
}
